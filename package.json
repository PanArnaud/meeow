{
  "name": "meeow",
  "version": "0.0.3",
  "description": "A dose of cute",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PanArnaud/meeow.git"
  },
  "keywords": [
    "cat",
    "cute",
    "kitten",
    "image",
    "url",
    "generator",
    "url-generator"
  ],
  "author": "Arnaud Panapadeatchy",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/PanArnaud/meeow/issues"
  },
  "homepage": "https://github.com/PanArnaud/meeow#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "prettier": "^1.15.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6"
  }
}
